{% extends 'base.html.twig' %}
{% trans_default_domain 'translation' %}

{% block title %}RoadSection{% endblock %}

{% block body %}
    <h1>RoadSection</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>{% trans %}Id{% endtrans %}</th>
                <td>{{ road_section.id }}</td>
            </tr>
            <tr>
                <th>{% trans %}Subunit{% endtrans %}</th>
                <td>{{ road_section.subunit }}</td>
            </tr>
            <tr>
                <th>{% trans %}SectionType{% endtrans %}</th>
                <td>{{ road_section.sectionType }}</td>
            </tr>
            <tr>
                <th>{% trans %}RoadSectionBegin{% endtrans %}</th>
                <td>{{ road_section.sectionBegin }}</td>
            </tr>
            <tr>
                <th>{% trans %}RoadSectionEnd{% endtrans %}</th>
                <td>{{ road_section.sectionEnd }}</td>
            </tr>
            <tr>
                <th>{% trans %}RoadSectionLength{% endtrans %}</th>
                <td>{{ road_section.sectionLength }}</td>
            </tr>
        </tbody>


    </table>

    <a class="btn btn-primary btn-lg" href="{{ path('winter_road_section') }}">{% trans %}back to list{% endtrans %}</a>

    <a class="btn btn-success btn-lg" href="{{ path('winter_road_section_edit', {'id': road_section.id}) }}">{% trans %}edit{% endtrans %}</a>

    {{ include('road_section_for_winter/_delete_form.html.twig') }}
{% endblock %}